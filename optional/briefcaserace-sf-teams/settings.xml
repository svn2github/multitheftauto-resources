	<!-- time the briefcase needs to be idle before it resets in seconds -->
	<idletime>300</idletime>

	<!-- the game speed (how fast everything moves) as a multiple of the normal game speed: e.g. 1 is normal, 0.5 is half speed, 10 is 10x speed (max) -->
