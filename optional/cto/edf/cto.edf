<def name="Capture the Orb">
	<element name="orb" friendlyname="Orb spawnpoint" instructions="Place your orb in a position that can be collected." icon="edf/orb.png">
		<data name="position" type="coord3d" instructions="XYZ position" default="0,0,0" />
		<marker size="0.5" type="corona" color="#ffff00ff" />
	</element>
	<element name="objective" friendlyname="Objective point" instructions="Place your objective point in a position that can be reached." icon="edf/objective.png">
		<data name="position" type="coord3d" default="0,0,0" />
		<marker size="3" type="cylinder" color="#9370dbaa" />
	</element>
	<element name="spawnpoint" friendlyname="Spawnpoint" icon="edf/spawnpoint.png">
		<data name="position" type="coord3d" default="0,0,0" />
		<data name="rotation" type="coord3d" default="0,0,0" />
		<data name="skin" type="skinID" default="0" />
		<ped model="!skin!" rotation="!rotation!"/>
	</element>
</def>